<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.houseapprules.ProfileController">
   <children>
      <Label layoutX="229.0" layoutY="49.0" text="Create Your Profile">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <Label layoutX="136.0" layoutY="100.0" text="Student Number" />
      <Label layoutX="136.0" layoutY="140.0" text="First Name" />
      <Label layoutX="136.0" layoutY="180.0" text="Last Name" />
      <Label layoutX="136.0" layoutY="220.0" text="Phone Number" />
      <Label layoutX="136.0" layoutY="260.0" text="Date Of Birth" />
      <TextField fx:id="studentNumber" layoutX="306.0" layoutY="100.0" />
      <TextField fx:id="firstName" layoutX="306.0" layoutY="140.0" />
      <TextField fx:id="lastName" layoutX="306.0" layoutY="180.0" />
      <TextField fx:id="phoneNumber" layoutX="306.0" layoutY="220.0" />
      <DatePicker fx:id="DOB" layoutX="306.0" layoutY="260.0" />
      <Button fx:id="saveBtn" layoutX="438.0" layoutY="341.0" mnemonicParsing="false" onAction="#save" text="Save" />
      <Button fx:id="cancelBtn" layoutX="89.0" layoutY="341.0" mnemonicParsing="false" onAction="#cancel" text="Cancel" textFill="#e40606" />
   </children>
</AnchorPane>
